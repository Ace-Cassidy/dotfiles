! Caps lock as Mode_switch
keycode 66 = Mode_switch

! Arrow Keys
keysym j = j J Left 
keysym l = l L Right 
keysym i = i I Up
keysym k = k K Down

! Other Navigation Keys
keysym u = u U Home
keysym o = o O End
keysym y = y Y Prior
keysym h = h h Next
keysym semicolon = semicolon colon Insert

! Both shift keys to as Caps_Lock
keysym Shift_R = Shift_R Caps_Lock Shift_R
keysym Shift_L = Shift_L Caps_Lock Shift_L
