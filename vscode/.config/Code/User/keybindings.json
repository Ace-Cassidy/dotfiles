// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+left",
        "command": "-cursorWordStartLeft",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+right",
        "command": "-cursorWordEndRight",
        "when": "textInputFocus"
    },
    {
        "key": "pageup",
        "command": "-cursorPageUp",
        "when": "textInputFocus"
    },
    {
        "key": "pagedown",
        "command": "-cursorPageDown",
        "when": "textInputFocus"
    },
    {
        "key": "pageup",
        "command": "cursorWordStartLeft",
        "when": "textInputFocus"
    },
    {
        "key": "pagedown",
        "command": "cursorWordEndRight",
        "when": "textInputFocus"
    }
   
]